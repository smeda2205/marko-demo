{
    "tags": {
        "gh-configure": {
            "renderer": "./taglib/gh-configure-tag",
            "attributes": {
                "layout": {
                    "type": "string",
                    "description": "header Layout Variation"
                },
                "footer-layout": {
                    "type": "string",
                    "description": "footer Layout Variation"
                },
                "var": {
                    "type": "string",
                    "description": "header Variation"
                },
                "category-id": {
                    "type": "integer",
                    "description": "Set category id for VNL"
                },
                "search-form-fields": {
                    "type": "expression",
                    "description": "hidden fields for search form"
                },
                "refined-categories": {
                    "type": "string",
                    "description": "refined categories"
                },
                "categories": {
                    "type": "string",
                    "description": "categories as shown in the categories select"
                },
                "search-form-action": {
                    "description": "search form action to be used"
                },
                "search-keywords": {
                    "description": "search keyword to display in search bar"
                },
                "all-categories-text": {
                    "description": "all categories text"
                },
                "ds-version": {
                    "type": "integer",
                    "description": "Design system version. Use this flag to get global css"
                },
                "*": {
                    "target-property": null
                }
            }
        },
        "gh-header": {
            "renderer": "./taglib/gh-header-tag"
        },
        "gh-footer": {
            "renderer": "./taglib/gh-footer-tag"
        },
        "gh-head-css": {
            "renderer": "./taglib/gh-head-css-tag"
        },
        "gh-body-js": {
            "renderer": "./taglib/gh-body-js-tag"
        },
        "gh-amp-css": {
            "renderer": "./taglib/gh-amp-css-tag"
        },
        "gh-amp-header": {
            "renderer": "./taglib/gh-amp-header-tag"
        },
        "gh-amp-footer": {
            "renderer": "./taglib/gh-amp-footer-tag"
        }
    }
}