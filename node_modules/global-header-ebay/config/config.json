{
    "globalheader": {
        "cache": {
            "enable": true,
            "max": 1000,          //Max cache size
            "maxAge": 1800000,        //Max Age (30 mins)
            "lastCleanup": 0,
            "cold": {
                "enable": false,
                "freeDelay": 360000,  // 6 min before releasing memory
                "flushDelay": 1000,
                "singleFile": false,
                "refreshInterval": 1800000 // every 30 min
            }
        },
        "fallback": true // fallback to static header/footer files
    },
    "services": {
        "globalheader": {
            "socketTimeout": 1000,
            "maxKeepAliveTime": 5000,
            "protocol": "http:",
            "hostname": "globalheader.vip",
            "basepath": "/header/json",
            "transport": "generic",
            "method": "GET",
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        },
        "globalfooter": {
            "socketTimeout": 1000,
            "maxKeepAliveTime": 5000,
            "protocol": "http:",
            "hostname": "globalheader.vip",
            "basepath": "/header/footer",
            "transport": "generic",
            "method": "GET",
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }
    },
    "@ConfigBean": {
        "global-header-ebay": {
            "common": [
                "globalheader"
            ]
        }
    },
    "@RaptorConfig": {
        "global-header-ebay": [{
            "target": "Global",
            "project": "node",
            "config": "global-header-ebay",
            "version": "0.3.32",
            "keyPath": "globalheader",
            "mapTo": "common"
        }]
    }
}
