<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta content="text/html; charset=us-ascii" http-equiv="Content-Type" />

  <title>Application Meta Information</title>
  <style title="currentStyle" type="text/css">
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * General page setup
 */
* {
margin: 0;
padding: 0;
}

#vi_page {
font: Arial, "Lucida Grande", Verdana, Helvetica, sans-serif;
margin: 0;
padding: 0;
color: #333333;
background-color: #ffffff;
}

#vi_page #container {
margin: 10px auto;
padding-left: 10px;
padding-right: 10px;
padding-bottom: 10px;
}

#vi_page #footer {
margin: 50px auto 0 auto;
padding: 0;
}

#vi_page #demo {
margin: 30px auto 0 auto;
}

#vi_page .demo_jui {
margin: 30px auto 0 auto;
}

#vi_page .big {
font-size: 1.3em;
font-weight: bold;
line-height: 1.6em;
color: #4E6CA3;
}

#vi_page .clear {
clear: both;
}

#vi_page h1 {
margin-top: 5px; // original was 2em
font-size: 1.3em;
font-weight: normal;
line-height: 1.6em;
color: #4E6CA3;
border-bottom: 1px solid #B0BED9;
clear: both;
}

#vi_page h2 {
font-size: 1.2em;
font-weight: normal;
line-height: 1.6em;
color: #4E6CA3;
clear: both;
}

#vi_page a:hover {
text-decoration: underline;
}

#vi_page .menu_td_button a, a:hover, a:link, a:visited, a:active {
text-decoration: none;
color: #FFFFFF;
}

#vi_page a {
color: #0063DC;
text-decoration: none;
}

#vi_page ul {
color: #4E6CA3;
}

#logoTable {
padding-top: 20px;
padding-left: 5px;
padding-bottom: 10px;
width:100%;
}

.css_right {
float: right;
}

.css_left {
float: left;
}

.error {
color: red;
font-weight: bold;
}

.nowrap {
white-space: nowrap;
}

.logo_image {
height: 46px;
width: 128px;
width:50%;
}

.meta_info{
font-family: Arial;
font-size: 20px;
color: #666;
line-height: 10px;
}

#topMenuDiv {
height: 29px;
display:  block;
overflow: hidden;
margin-bottom: 20px;
clear: both;
}

.space20v,
.space20h,
.space9h {
border: 0;
background-color: transparent;
clear: both;
display: block;
}

.space20v {
height: 20px;
}

.space9h {
width: 9px;
float: left:
}

.space20h {
width: 20px;
float: left:
}

.sub_title,
.main_sub_title {

}

.main_sub_title {
font-weight: bold;
}

.table_title {
font-family: Arial;
text-align: left;
font-size: 13px;
font-weight: bold;
width: 100%;
height: 30px;
line-height: 30px;
padding-left: 10px;
padding-right: 20px;
background-color: #87CEFA;
clear: left;
}

table.display thead th {
        padding: 3px 0px 3px 10px;
        font-family: Arial;
        font-size: 11px;
        color: rgb(51,51,51);
        border: 1px solid rgb(211,211,211);
        font-weight: normal;
        background-color: #E6E6E6;
}


.page_title,
.page_subtitle {
font-family: Arial;
text-align: left;
padding-top: 4px;
padding-bottom: 4px;
}

.page_title {
font-size: 13px;
font-weight: bold;
}

.page_subtitle {
font-size: 12px;
}

.page_nav {
border: 1px solid #999999;
padding-left: 20px;
padding-right: 20px;
padding-top: 15px;
padding-bottom: 20px;
background-size: 100% 100%;
font-family: Arial;
font-size: 12px;
color: #333333;
vertical-align: middle;
white-space: nowrap;
overflow: hidden;
display: block;
min-width: 500px;
margin-bottom: 20px;
}

.page_nav div {
border: 0px;
float: left;
display: inline-block;
padding-right: 15px;
}

.page_nav ul {
margin: 0; padding: 0; float: left;
}

.page_nav ul li {
display: inline;
}

.page_nav > li {
float: left;
}

.page_nav ul a {
float: left;
text-decoration: none;
background-repeat: no-repeat;
background-position: left;
padding-right: 15px;
padding-left: 25px;
}

.list_items {
background-color: transparent;
font-family: Arial;
font-size: medium;
font-weight: bold;
list-style-type: square;
color: #224680;
}


/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * DataTables row classes
 */

 .DataTables_sort_wrapper {
  text-align: left;
  padding-left: 5px;
 }

table.display td, table.display td pre {
font-family: Arial;
font-size: 12px;
color: #333333;
}

tr.even, tr.even td.sorting_1 {
background-color: #F3F3F3;
}

tr.odd, tr.odd td.sorting_1 {
background-color: white;
}

</style>
</head>

<body id="vi_page">
  <table id="logoTable">
    <tbody>
      <tr>
        <td colspan="99" valign="middle">

          <div class="logo_image"><img src="http://www.ebayinc.com/sites/default/files/logo.png"/>
            <span class="meta_info">Application Meta Information</span>
          </div>
        </td>

      </tr>

      <tr>
        <td class="space9h">&nbsp;</td>

        <td>
          <span class="main_sub_title">Host: {hostname}/({ip})</span>
        </td>
      </tr>
    </tbody>
  </table>

  <div id="container">
    <div class="page_nav_wrapper">
      <div class="page_nav">
        <div>
          Go to
        </div>

        <ul>
          <li><a href="#Component_1">Host Information</a></li>

          <li><a href="#Component_2">Application Build Information</a></li>

          <li><a href="#Component_4">Middleware</a></li>

          <li><a href="#Component_5">Routes</a></li>

          <li><a href="#Component_6">Services</a></li>

          <li><a href="#Component_7">Packages</a></li>
        </ul>
      </div>
    </div><a name="Component_1"></a>

    <div id="hostInfoTableId_wrapper" class="dataTables_wrapper" role="grid">
      <div class=
      "fg-toolbar ui-toolbar ui-widget-header ui-corner-tl ui-corner-tr ui-helper-clearfix">
      <div class="table_title" id="hostInfoTableId_title">
          Application Information
        </div>

      </div>

      <table style="width: 1391px;" aria-describedby="hostInfoTableId_info" class=
      "display componentTable dataTable" id="hostInfoTableId">
        <thead>
          <tr role="row">
            <th aria-label="Metric: colspan="1" rowspan="1" aria-controls="hostInfoTableId" tabindex=
            "0" role="columnheader" style="width: 476px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Metric
              </div>
            </th>

            <th aria-label="Value: colspan="1"
            rowspan="1" aria-controls="hostInfoTableId" tabindex="0" role="columnheader"
            style="width: 874px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Value
              </div>
            </th>
          </tr>
        </thead>

        <tbody aria-relevant="all" aria-live="polite" role="alert">
          <tr class="odd">
            <td class=" sorting_1">Application Name</td>

            <td class="">{appInfo.appName}</td>
          </tr>
          <tr class="even">
            <td class=" sorting_1">Application Version</td>

            <td class="">{appInfo.appVersion}</td>
          </tr>

          <tr class="odd">
            <td class=" sorting_1">Host</td>

            <td class="">{hostname}/({ip})</td>
          </tr>

          <tr class="even">
            <td class=" sorting_1">Framework Name</td>

            <td class="">{appInfo.frameworkName}</td>
          </tr>

          <tr class="odd">
            <td class=" sorting_1">Framework Version</td>

            <td class="">{appInfo.frameworkVersion}</td>
          </tr>

          <tr class="even">
            <td class=" sorting_1">Up time</td>

            <td class="">{processInfo.uptime}</td>
          </tr>

          <tr class="odd">
            <td class=" sorting_1">OS</td>

            <td class="">{processInfo.platform} {processInfo.architecture}</td>
          </tr>

          <tr class="even">
            <td class=" sorting_1">NODE_ENV</td>

            <td class="">{processInfo.nodeEnv}</td>
          </tr>

          <tr class="odd">
            <td class=" sorting_1">DEPLOY_ENV</td>

            <td class="">{processInfo.deployEnv}</td>
          </tr>

          <tr class="even">
            <td class=" sorting_1">Memory Available</td>

            <td class="">{memory.total}</td>
          </tr>

          <tr class="odd">
            <td class=" sorting_1">Memory Used</td>

            <td class="">{memory.used}</td>
          </tr>

          <tr class="even">
            <td class=" sorting_1">Resource Base URL</td>

            <td class="">{resourceBaseUrl}</td>
          </tr>

        </tbody>
      </table>

    </div>

    <div class="space20v"></div><a name="Component_2"></a>

    <div id="comp_table_2_wrapper" class="dataTables_wrapper" role="grid">
      <div class=
      "fg-toolbar ui-toolbar ui-widget-header ui-corner-tl ui-corner-tr ui-helper-clearfix">
      <div class="table_title" id="comp_table_2_title">
          Application Build Information
        </div>

      </div>

      <table style="width: 1391px;" aria-describedby="comp_table_2_info" class=
      "display componentTable dataTable" id="comp_table_2">
        <thead>
          <tr role="row">
            <th aria-label="Metric: aria-sort=
            "ascending" colspan="1" rowspan="1" aria-controls="comp_table_2" tabindex="0"
            role="columnheader" style="width: 467px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Metric
              </div>
            </th>

            <th aria-label="Value: activate to sort column ascending" colspan="1"
            rowspan="1" aria-controls="comp_table_2" tabindex="0" role="columnheader"
            style="width: 864px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Value
              </div>
            </th>
          </tr>
        </thead>

        <tbody aria-relevant="all" aria-live="polite" role="alert">
          <tr class="even">
            <td class=" sorting_1">Build Id</td>

            <td class="">{appInfo.buildId}</td>
          </tr>

          <tr class="odd">
            <td class=" sorting_1">Build Timestamp</td>

            <td class="">{appInfo.buildTimestamp}</td>
          </tr>
          <tr class="even">
            <td class=" sorting_1">Git Url</td>

            <td class="">{scm.gitUrl}</td>
          </tr>

          <tr class="odd">
            <td class=" sorting_1">Git Branch</td>

            <td class="">{scm.gitBranch}</td>
          </tr>
          <tr class="even">
            <td class=" sorting_1">Git Commit</td>

            <td class="">{scm.gitCommit}</td>
          </tr>
        </tbody>
      </table>

      </div>
    </div>

    <div class="space20v"></div><a name="Component_4"></a>

    <div id="comp_table_4_wrapper" class="dataTables_wrapper" role="grid">
      <div class=
      "fg-toolbar ui-toolbar ui-widget-header ui-corner-tl ui-corner-tr ui-helper-clearfix">
      <div class="table_title" id="comp_table_4_title">
          Middleware
        </div>
      </div>

      <table style="width: 1391px;" aria-describedby="comp_table_4_info" class=
      "display componentTable dataTable" id="comp_table_4">
        <thead>
          <tr role="row">
            <th aria-label="Metric: aria-sort=
            "ascending" colspan="1" rowspan="1" aria-controls="comp_table_4" tabindex="0"
            role="columnheader" style="width: 457px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Name
              </div>
            </th>

            <th aria-label="Value: activate to sort column ascending" colspan="1"
            rowspan="1" aria-controls="comp_table_4" tabindex="0" role="columnheader"
            style="width: 864px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Value
              </div>
            </th>
          </tr>
        </thead>

        <tbody aria-relevant="all" aria-live="polite" role="alert">
          {#middleware}
          <tr class="even">
            <td class=" sorting_1">{.}</td>

            <td class=""></td>
          </tr>
          {/middleware}
        </tbody>
      </table>
    </div>

    <div class="space20v"></div><a name="Component_5"></a>

    <div id="comp_table_5_wrapper" class="dataTables_wrapper" role="grid">
      <div class=
      "fg-toolbar ui-toolbar ui-widget-header ui-corner-tl ui-corner-tr ui-helper-clearfix">
      <div class="table_title" id="comp_table_5_title">
          Routes
        </div>
      </div>

      <table style="width: 1391px;" aria-describedby="comp_table_5_info" class=
      "display componentTable dataTable" id="comp_table_5">
        <thead>
          <tr role="row">
            <th aria-label="Metric:  aria-sort=
            "ascending" colspan="1" rowspan="1" aria-controls="comp_table_5" tabindex="0"
            role="columnheader" style="width: 285px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Path
              </div>
            </th>

            <th aria-label="Value:  colspan="1"
            rowspan="1" aria-controls="comp_table_5" tabindex="0" role="columnheader"
            style="width: 1065px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Methods
              </div>
            </th>
          </tr>
        </thead>

        <tbody aria-relevant="all" aria-live="polite" role="alert">
          {#routes}
          <tr class="even">
            <td class=" sorting_1">{.name}</td>

            <td class="">{.methods}</td>
          </tr>
          {/routes}

        </tbody>
      </table>

    </div>

    <div class="space20v"></div><a name="Component_6"></a>

    <div id="comp_table_6_wrapper" class="dataTables_wrapper" role="grid">
      <div class=
      "fg-toolbar ui-toolbar ui-widget-header ui-corner-tl ui-corner-tr ui-helper-clearfix">
      <div class="table_title" id="comp_table_6_title">
          Services
        </div>

      </div>

      <table style="width: 1391px;" aria-describedby="comp_table_6_info" class=
      "display componentTable dataTable" id="comp_table_6">
        <thead>
          <tr role="row">
            <th aria-label="Metric: aria-sort=
            "ascending" colspan="1" rowspan="1" aria-controls="comp_table_6" tabindex="0"
            role="columnheader" style="width: 285px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Service
              </div>
            </th>

            <th aria-label="Value: colspan="1"
            rowspan="1" aria-controls="comp_table_6" tabindex="0" role="columnheader"
            style="width: 1065px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Host:port
              </div>
            </th>
          </tr>
        </thead>

        <tbody aria-relevant="all" aria-live="polite" role="alert">
          {#services}
          <tr class="even">
            <td class=" sorting_1">{.service}</td>

            <td class="">{.host}:{.port}</td>
          </tr>
          {/services}
        </tbody>
      </table>

    </div>

    <div class="space20v"></div><a name="Component_7"></a>

    <div id="comp_table_7_wrapper" class="dataTables_wrapper" role="grid">
      <div class=
      "fg-toolbar ui-toolbar ui-widget-header ui-corner-tl ui-corner-tr ui-helper-clearfix">
      <div class="table_title" id="comp_table_7_title">
          Packages
        </div>

        </div>
      </div>

      <table style="width: 1391px;" aria-describedby="comp_table_7_info" class=
      "display componentTable dataTable" id="comp_table_7">
        <thead>
          <tr role="row">
            <th aria-label="Metric:  aria-sort=
            "ascending" colspan="1" rowspan="1" aria-controls="comp_table_7" tabindex="0"
            role="columnheader" style="width: 285px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Package
              </div>
            </th>

            <th aria-label="Value:  colspan="1"
            rowspan="1" aria-controls="comp_table_7" tabindex="0" role="columnheader"
            style="width: 1065px;" class="left ui-state-default">
              <div class="DataTables_sort_wrapper">
                Version
              </div>
            </th>
          </tr>
        </thead>

        <tbody aria-relevant="all" aria-live="polite" role="alert">
          {#pkg}
          <tr class="even">
            <td class=" sorting_1">{name}</td>

            <td class="">{version}</td>
          </tr>
          {/pkg}
        </tbody>
      </table>

    </div>

    <div class="space20v"></div>
  </div>

</body>
</html>

