{
    "<gh-configure>": {
        "renderer": "./src/gh-configure-tag",
        "@layout": {
            "type": "string",
            "description": "header Layout Variation"
        },
        "@footer-layout": {
            "type": "string",
            "description": "footer Layout Variation"
        },
        "@var": {
            "type": "string",
            "description": "header Variation"
        },
        "@category-id": {
            "type": "integer",
            "description": "Set category id for VNL"
        },
        "@search-form-fields": {
            "type": "expression",
            "description": "hidden fields for search form"
        },
        "@refined-categories": {
            "type": "string",
            "description": "refined categories"
        },
        "@categories": {
            "type": "string",
            "description": "categories as shown in the categories select"
        },
        "@search-form-action": {
            "description": "search form action to be used"
        },
        "@search-keywords": {
            "description": "search keyword to display in search bar"
        },
        "@all-categories-text": {
            "description": "all categories text"
        },
        "@ds-version": {
            "type": "integer",
            "description": "Design system version. Use this flag to get global css"
        },
        "@*": {
            "target-property": null
        },
        "@application-page-id": {
            "type": "string",
            "description": "Application page id"
        }
    },
    "<gh-header>": {
        "renderer": "./src/gh-header-tag"
    },
    "<gh-footer>": {
        "renderer": "./src/gh-footer-tag"
    },
    "<gh-head-css>": {
        "renderer": "./src/gh-head-css-tag"
    },
    "<gh-body-js>": {
        "renderer": "./src/gh-body-js-tag"
    },
    "<gh-amp-head-js>": {
        "renderer": "./src/gh-amp-head-js-tag"
    },
    "<gh-amp-inline-css>": {
        "renderer": "./src/gh-amp-inline-css-tag"
    },
    "<gh-amp-css>": {
        "renderer": "./src/gh-amp-css-tag"
    },
    "<gh-amp-header>": {
        "renderer": "./src/gh-amp-header-tag"
    },
    "<gh-amp-footer>": {
        "renderer": "./src/gh-amp-footer-tag"
    }
}
