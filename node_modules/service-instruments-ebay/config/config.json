{
    "service-instruments-ebay": {

        "default-instruments": [
            "https",
            "ep",
            "tracking",
            "outbound-context",
            "oauth",
            "circuit",
            "retry",
            "cal",
            "metrics"
        ],

        "instruments-metadata": {
            "config": {
                "module": "service-instruments-ebay/config",
                "priority": 5
            },
            "cal": {
                "module": "service-instruments-ebay/cal",
                "priority": 10
            },
            "circuit": {
                "module": "service-instruments-ebay/circuit",
                "priority": 15
            },
            "cache": {
                "module": "service-instruments-ebay/cache",
                "priority": 20
            },
            "config-bean": {
                "module": "service-instruments-ebay/noop",
                "priority": 20
            },
            "retry": {
                "module": "service-instruments-ebay/retry",
                "priority": 30
            },
            "error-handler": {
                "module": "service-instruments-ebay/circuit",
                "priority": 25
            },
            "soap": {
                "module": "service-instruments-ebay/soap",
                "priority": 50
            },
            "oauth": {
                "module": "service-instruments-ebay/oauth",
                "priority": 50
            },
            "outbound-context": {
                "module": "service-instruments-ebay/outbound-context",
                "priority": 60
            },
            "tracking": {
                "module": "service-instruments-ebay/tracking",
                "priority": 70
            },
            "ep": {
                "module": "service-instruments-ebay/ep",
                "priority": 80
            },
            "https": {
                "module": "service-instruments-ebay/https",
                "priority": 90
            },
            "cookies": {
                "module": "service-instruments-ebay/cookies",
                "priority": 100
            },
            "compress": {
                "module": "service-instruments-ebay/compress",
                "priority": 1000
            },
            "metrics": {
                "module": "service-instruments-ebay/metrics",
                "priority": 1010
            },
            "generic": {
                "module": "service-instruments-ebay/transports/http-transport",
                "transport": true
            },
            "rest": {
                "module": "service-instruments-ebay/transports/http-transport",
                "transport": true
            },
            "sifr": {
                "module": "service-instruments-ebay/transports/sifr-transport",
                "transport": true
            },
            "xhr": {
                "module": "service-instruments-ebay/transports/xhr-transport",
                "transport": true
            },
            "sse": {
                "module": "service-instruments-ebay/transports/sse",
                "transport": true
            },
            "mje": {
                "module": "service-instruments-ebay/transports/mje",
                "transport": true
            },
            "simple-chunk": {
                "module": "service-instruments-ebay/transports/simple-chunk",
                "transport": true
            },
            "json-stream": {
                "module": "service-instruments-ebay/transports/json-stream",
                "transport": true
            },
            "mock-transport": {
                "module": "service-instruments-ebay/transports/mock-transport",
                "transport": true
            }
        }
    },

    "SvcVipMappingConfig": {
        "recognizedDomains": [".stratus", ".vip"],
        "patternListForMapping": [".ebay.com", ".stratus.ebay.com"],
        "vipMapping": {
            "raptordds.vip": "www.raptor-dds.stratus.ebay.com",
            "seskeysvc.vip": "seskeysvc.vip.ebay.com",
            "sessionkeysvc.vip": "seskeysvc.vip.ebay.com",
            "resreposvc.vip": "resreposvc.vip.ebay.com",
            "esams.vip": "esams.vip.ebay.com",
            "pes.vip": "pes.vip.ebay.com",
            "idauth.vip": "idauth.vip.ebay.com",
            "oauthclnt.vip": "oauthclnt.vip.ebay.com",
            "globalheader.vip": "www.gh.stratus.ebay.com",
            "iafservice.vip": "soaident.vip.ebay.com",
            "epsvc.vip": "www.epsvc.stratus.ebay.com",
            "authzsvc.vip": "authzsvc.vip.ebay.com"
        }
    },

    "@ConfigBean": {
        "service-instruments-ebay": {
            "common": [                
                "SvcVipMappingConfig"
            ]
        }
    },
    "@RaptorConfig": {
          "service-instruments-ebay": [
              {
                  "target": "Global",
                  "project": "Raptor",
                  "config": "SvcVipMappingConfig",
                  "version": "1.0.0",
                  "keyPath": "SvcVipMappingConfig"
              }
          ]
      }


}
