<Component name="Configuration Change History" alias="configurationchangehistory" status="created">
    <% var components = modifiedAttributes %>
    <% for(var i=0; i<components.length; i++){
        var comp = components[i] %>

        <Properties>
            <Property name="ConfigCategoryId"><%=comp.id%></Property>
            <Property name="EndTime">
            <![CDATA[ <%=new Date(comp.lastUpdated)%> ]]>
            </Property>
            <Property name="Message">
            <![CDATA[ n/a ]]>
            </Property>
            <Property name="NewValue">
            <![CDATA[ <%=comp.newValue%> ]]>
            </Property>
            <Property name="OldValue">
            <![CDATA[ <%=comp.oldValue%> ]]>
            </Property>
            <Property name="PropertyName">
            <![CDATA[ <%=comp.name%> ]]>
            </Property>
            <Property name="ResponsibleParty">
            <![CDATA[ n/a ]]>
            </Property>
            <Property name="StackTrace">
            <![CDATA[ n/a ]]>
            </Property>
            <Property name="StartTime">
            <![CDATA[ <%=new Date(comp.lastUpdated)%> ]]>
            </Property>
            <Property name="Status">
            <![CDATA[ success ]]>
            </Property>
        </Properties>

    <% } %>

</Component>
