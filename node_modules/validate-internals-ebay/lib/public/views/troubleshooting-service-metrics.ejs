<Component name="Service Stats" alias="Service Stats" status="OK">

    <% serviceMetrics.forEach(function forEach(metrics) { %>

        <Properties>
            <Property name="Name"><![CDATA[<%=metrics.name%>]]></Property>

           <% var props = metrics.properties;
             for(var i=0; i < props.length; i++) {
                var prop = props[i];
            %>
                <Property name="<%=prop.name%>"><![CDATA[<%=prop.value%>]]></Property>

            <% } %>
        </Properties>

    <% }); %>

</Component>
