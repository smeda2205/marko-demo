<?xml version="1.0" encoding="UTF-8"?>
<ViewConfigCategory xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" host="<%=host%>/<%=ipAddress%>" client-ip="<%=clientIpAddress%>">
   <ConfigCategory id="<%=id%>" alias="<%=alias%>" group="<%=group%>" category-desc="<%=description%>" persist-updates="false" timestamp="<%=time%>" Validator="com.ebay.kernel.bean.configuration.adapter.ConfigManagementValidatorAdapter" site-ops-command="true" persistent-location="" isDynamic="false">
    <% for(var i=0; i<attributes.length; i++) {
        var attr = attributes[i] %>
        <attribute name="<%=attr.name%>" value="<%=attr.value%>" description="<%=attr.description%>" isReadable="<%=attr.readable%>" isWritable="<%=attr.writable%>" type="<%=attr.type%>" persist-policy="Default" />
    <% } %>
    </ConfigCategory>
</ViewConfigCategory>