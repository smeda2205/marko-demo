<!doctype html>
<html>
<head>
    <title>npm ls</title>
    <style>
        body {
            font-family: 'Courier New';
            font-size: 10pt;
        }
    </style>
</head>
<body>

<h2>npm ls</h2>

<%
function inspect(node){
    var html = '<div><span>' + node.name + '@'+ node.version;
    if(node.path){
        html += '#' + node.path
    }
    html += '</span><ul>';

    (node.children || []).forEach(function(child){
        html += '<li>' + inspect(child) + '</li>'
        });
    html += '</ul></div>';

    return html;
}
%>

<%-inspect(tree)%>

</body>
</html>