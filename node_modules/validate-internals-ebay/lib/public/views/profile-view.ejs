<% if(!forceXml){ %> <?xml-stylesheet type="text/xsl" href="resources/xsl/Profile.xsl"?><% } else { %><?xml version="1.0" encoding="UTF-8"?><% } %>
<ProfileView host="<%=host%>/<%=ipAddress%>" client-ip="<%=clientIpAddress%>" uiTheme="../css/raptor_page.css" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="resources/xsd/ViewConfigCategory.xsd">

    <%- include('header'); %>

    <ProfileRequest type="<%=type%>" file="<%=file%>" duration="<%=duration%>" view="<%=view%>" base="<%=base%>" workersNumber="<%=workersNumber%>" worker="<%=worker%>">
        <% if (status) { %>
            <Status left="<%=status.left%>"
                requested="<%=status.requested%>"
                interval="<%=status.interval%>"
                intervalMs="<%=status.intervalMs%>"
                units="<%=status.units%>"
                nextDump="<%=status.nextDump%>"
                lastDump="<%=status.lastDump%>" />
        <% } %>
    </ProfileRequest>
    <% if (profiles) { %>
        <Profiles>
            <% profiles.forEach(function forEach(profile) { %>
                <Profile file="<%=profile.file%>" date="<%=profile.date%>" size="<%=profile.size%>" />
            <% }); %>
        </Profiles>
    <% } %>
</ProfileView>
