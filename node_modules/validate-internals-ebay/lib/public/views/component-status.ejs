<% if(!forceXml){ %> <?xml-stylesheet type="text/xsl" href="resources/xsl/ValidateInternals.xsl"?><% } else { %><?xml version="1.0" encoding="UTF-8"?><% } %>
<ValidateInternals xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" host="<%=host%>/<%=ipAddress%>" client-ip="<%=clientIpAddress%>" uiTheme="../css/raptor_page.css" xsi:noNamespaceSchemaLocation="resources/xsd/ValidateInternals.xsd" more-info="HeaderPH">

<%- include('header'); %>

<Component name="<%=component.name%>" alias="<%=component.alias%>" status="created">

<Properties>
   <% var props = component.properties
     for(var i=0; i<props.length; i++){
        var prop = props[i]
    %>
        <Property <% if (prop.color) { %>warningLevel="<%=prop.color%>"<% } %>  <% if (prop.href) { %>keyHref="<%=prop.href%>"<% } %> name="<%=prop.name%>"><![CDATA[<%-prop.value%>]]></Property>

    <% } %>
</Properties>

%>

</Component>

</ValidateInternals>
