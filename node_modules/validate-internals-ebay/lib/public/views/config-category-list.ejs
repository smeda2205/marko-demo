<% if(!forceXml){ %><?xml-stylesheet type="text/xsl" href="resources/xsl/ConfigCategoryList.xsl"?><% } else { %><?xml version="1.0" encoding="UTF-8"?><% } %>
<ConfigCategoryList host="<%=host%>/<%=ipAddress%>" client-ip="<%=clientIpAddress%>" uiTheme="../css/raptor_page.css" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="resources/xsd/ConfigCategoryList.xsd" view="group">

    <%- include('header'); %>

    <% for(var i=0; i<configCategoryGroups.length; i+=1){
        var group = configCategoryGroups[i] %>
        <ConfigCategoryGroup name="<%=group.name%>">
        <% for(var j=0; j<group.configCategorys.length; j+=1){
            var configCategory = group.configCategorys[j] %>
            <ConfigCategory id="<%=configCategory.name || configCategory.id%>"/>
        <% } %>
        </ConfigCategoryGroup>
    <% } %>

</ConfigCategoryList>
