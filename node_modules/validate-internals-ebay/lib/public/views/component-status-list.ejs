<% if(!forceXml){ %><?xml-stylesheet type="text/xsl" href="resources/xsl/ComponentStatusList.xsl"?><% } else { %><?xml version="1.0" encoding="UTF-8"?><% } %>
<ComponentStatusList host="<%=host%>/<%=ipAddress%>" client-ip="<%=clientIpAddress%>" uiTheme="../css/raptor_page.css" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="resources/xsd/ComponentStatusList.xsd" view="group">

	<%- include('header'); %>

	<% for(var i=0; i<components.length; i++){var comp = components[i] %>
		<Component name="<%=comp.name%>" alias="<%=comp.name%>" status="created"/>
	<% } %>

</ComponentStatusList>
